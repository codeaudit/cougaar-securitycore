<?xml version='1.0'?>
<!--DAML+OIL Language, version 03/2001-->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
         xmlns:daml="http://www.daml.org/2001/03/daml+oil#" 
         xmlns="http://ontology.coginst.uwf.edu/Ultralog/UltralogAction.daml#">
    
    <daml:Ontology rdf:about="">
        <daml:versionInfo>
            $ http://ontology.coginst.uwf.edu/Ultralog/UltralogAction.daml $
        </daml:versionInfo>
        <rdfs:comment>
            An ontology created by Andrzej Uszok (auszok@ai.uwf.edu).
        </rdfs:comment>
        <daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Action.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml"/>
    </daml:Ontology>

<!-- Section with property definitions--> 

    <daml:ObjectProperty rdf:ID="usedAuthenticationLevel">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedOn"/> 
        <rdfs:domain rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#Action"/> 
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#AuthenticationLevel"/> 
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="usedProtectionLevel">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedOn"/>
        <rdfs:domain rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#EncryptedCommunicationAction"/> 
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#ProtectionLevel"/> 
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="accessedServlet">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#accessedEntity"/>
        <rdfs:domain rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#AccessAction"/> 
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#Servlet"/> 
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="hasSubject">
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedOn"/>
        <rdfs:domain rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#CommunicationAction"/> 
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#ULContentValue"/> 
    </daml:ObjectProperty>


    <daml:ObjectProperty rdf:ID="roleOfPluginAccessingBlackBoard">
        <rdfs:domain rdf:resource="#BlackBoardAccess"/> 
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#PlugInRoles"/> 
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="blackBoardAccessMode">
        <rdfs:domain rdf:resource="#BlackBoardAccess"/> 
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#BlackBoardAccessMode"/> 
    </daml:ObjectProperty>

<!-- Section with class definitions--> 


     <daml:Class rdf:ID="BlackBoardAccess">
        <rdfs:subClassOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#ResourceAction" />
        <rdfs:subClassOf>
           <daml:Restriction>
              <daml:onProperty rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedOn" />
              <daml:toClass rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#BlackBoardObjects" />
           </daml:Restriction>
        </rdfs:subClassOf>
     </daml:Class>

</rdf:RDF>
