<?xml version='1.0'?>
<!--DAML+OIL Language, version 03/2001-->
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
         xmlns:ultralogEntity="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#"
         xmlns="http://ontology.coginst.uwf.edu/Ultralog/Names/EntityClasses.daml#">
    
    <daml:Ontology rdf:about="">
        <daml:versionInfo>
            $ http://ontology.coginst.uwf.edu/Ultralog/Names/EntityClasses.daml $
        </daml:versionInfo>
        <rdfs:comment>
            An ontology created by Andrzej Uszok (auszok@ai.uwf.edu).
        </rdfs:comment>
        <daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Group.daml" />
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/Names/EntityInstances.daml"/>
    </daml:Ontology>

    <daml:Class rdf:ID="CAService">
        <rdfs:subClassOf rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#Service"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="http://ontology.coginst.uwf.edu/Group.daml#hasMember"/>
                <daml:toClass rdf:resource="#CAOperation"/>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="CAOperation">
        <rdfs:subClassOf rdf:resource="http://ontology.coginst.uwf.edu/Ultralog/UltralogEntity.daml#Operation"/>
    </daml:Class>

    <daml:Class rdf:ID="CAWriteOperation">
        <rdfs:subClassOf rdf:resource="#CAOperation"/>
    </daml:Class>

    <daml:Class rdf:ID="CAReadOperation">
        <rdfs:subClassOf rdf:resource="#CAOperation"/>
    </daml:Class>

    <daml:Class rdf:ID="CARequestOperation">
        <rdfs:subClassOf rdf:resource="#CAOperation"/>
    </daml:Class>

    <daml:Class rdf:about="#CAOperation">
        <daml:disjointUnionOf rdf:parseType="daml:collection">
            <daml:Class rdf:about="#CAWriteOperation"/>
            <daml:Class rdf:about="#CAReadOperation"/>
            <daml:Class rdf:about="#CARequestOperation"/>
        </daml:disjointUnionOf>
    </daml:Class>

</rdf:RDF>