<?xml version='1.0'?>
<!--DAML+OIL Language, version 03/2001-->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
         xmlns:daml="http://www.daml.org/2001/03/daml+oil#" 
         xmlns="http://ontology.coginst.uwf.edu/Action.daml#">
    
    <daml:Ontology rdf:about="">
        <daml:versionInfo>
            $ http://ontology.coginst.uwf.edu/Action.daml $
        </daml:versionInfo>
        <rdfs:comment>
            An ontology created by Andrzej Uszok (auszok@ai.uwf.edu).
        </rdfs:comment>
        <daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Group.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Entity.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Policy.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Message.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Place.daml"/>
    </daml:Ontology>

<!--Section with property definitions-->

    <daml:ObjectProperty rdf:ID="performedBy">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:domain rdf:resource="#Action"/>
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml#Actor"/>
    </daml:ObjectProperty>

    <rdfs:Property rdf:ID="hasContext">
        <rdfs:domain rdf:resource="#Action"/>
    </rdfs:Property>

    <daml:ObjectProperty rdf:ID="performedOn">
        <rdfs:subPropertyOf rdf:resource="#hasContext"/>
        <rdfs:domain rdf:resource="#Action"/>
        <rdfs:range rdf:resource="#Target"/>
    </daml:ObjectProperty>
       
    <daml:ObjectProperty rdf:ID="carriesMessage">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="#performedOn"/>
        <rdfs:domain rdf:resource="#CommunicationAction"/>
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Message.daml#Message"/>
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="hasDestination">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="#performedOn"/>
        <rdfs:domain rdf:resource="#CommunicationAction"/>
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml#Actor"/>
    </daml:ObjectProperty>

   <daml:ObjectProperty rdf:ID="replyTo">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="#performedOn"/>
        <rdfs:domain rdf:resource="#ResponseAction"/>
        <rdfs:range rdf:resource="#RequestAction"/>
    </daml:ObjectProperty>
        
    <daml:ObjectProperty rdf:ID="movingTo">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="#performedOn"/>
        <rdfs:domain rdf:resource="#MobilityAction"/>
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Place.daml#Place"/>
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="accessedEntity">
        <rdfs:subPropertyOf rdf:resource="#performedOn"/>
        <rdfs:domain rdf:resource="#AccessAction"/>
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Entity.daml#Entity"/>
    </daml:ObjectProperty>

<!--Section with class definitions-->

    <daml:Class rdf:ID="Target">
        <daml:unionOf rdf:parseType="daml:collection">
            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Group.daml#Group"/> 
            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Actor.daml#Actor"/> 
            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Entity.daml#Entity"/> 
            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Policy.daml#Policy"/>
            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Message.daml#Message"/>
            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Place.daml#Place"/> 
            <daml:Class rdf:about="#Action"/> 
        </daml:unionOf>
    </daml:Class>

    <daml:Class rdf:ID="Action"/>

    <daml:Class rdf:ID="CommunicationAction">
        <rdfs:subClassOf rdf:resource="#Action"/>
    </daml:Class>

    <daml:Class rdf:ID="EncryptedCommunicationAction">
        <rdfs:subClassOf rdf:resource="#CommunicationAction"/>
    </daml:Class>

    <daml:Class rdf:ID="NonEncryptedCommunicationAction">
        <rdfs:subClassOf rdf:resource="#CommunicationAction"/>
    </daml:Class>

    <daml:Class rdf:about="#CommunicationAction">
        <daml:disjointUnionOf rdf:parseType="daml:collection">
            <daml:Class rdf:about="#EncryptedCommunicationAction"/> 
            <daml:Class rdf:about="#NonEncryptedCommunicationAction"/> 
        </daml:disjointUnionOf>
    </daml:Class>

    <daml:Class rdf:ID="SignedCommunicationAction">
        <rdfs:subClassOf rdf:resource="#CommunicationAction"/>
    </daml:Class>

    <daml:Class rdf:ID="NonSignedCommunicationAction">
        <rdfs:subClassOf rdf:resource="#CommunicationAction"/>
    </daml:Class>

    <daml:Class rdf:about="#CommunicationAction">
        <daml:disjointUnionOf rdf:parseType="daml:collection">
            <daml:Class rdf:about="#SignedCommunicationAction"/> 
            <daml:Class rdf:about="#NonSignedCommunicationAction"/> 
        </daml:disjointUnionOf>
    </daml:Class>

    <daml:Class rdf:ID="RequestAction">
        <rdfs:subClassOf rdf:resource="#CommunicationAction"/>
    </daml:Class>

    <daml:Class rdf:ID="ResponseAction">
        <rdfs:subClassOf rdf:resource="#CommunicationAction"/>
    </daml:Class>

    <daml:Class rdf:ID="ProposeAction">
        <rdfs:subClassOf rdf:resource="#RequestAction"/>
    </daml:Class>

    <daml:Class rdf:ID="ApproveAction">
        <rdfs:subClassOf rdf:resource="#ResponseAction"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#replyTo"/>
                <daml:toClass rdf:resource="#ProposeAction"/>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="MonitorAction">
        <rdfs:subClassOf rdf:resource="#Action"/>
    </daml:Class>

    <daml:Class rdf:ID="QueryAction">
        <rdfs:subClassOf rdf:resource="#Action"/>
    </daml:Class>

    <daml:Class rdf:ID="MobilityAction">
        <rdfs:subClassOf rdf:resource="#Action"/>
    </daml:Class>

    <daml:Class rdf:ID="ResourceAction">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#performedOn"/>
                <daml:hasClass rdf:resource="http://ontology.coginst.uwf.edu/Entity.daml#Entity"/>
            </daml:Restriction>
         </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="AccessAction">
        <rdfs:subClassOf rdf:resource="#Action"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#performedOn"/>
                <daml:hasClass rdf:resource="http://ontology.coginst.uwf.edu/Entity.daml#Entity"/>
            </daml:Restriction>
         </rdfs:subClassOf>
    </daml:Class>

</rdf:RDF>
