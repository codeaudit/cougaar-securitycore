<?xml version='1.0'?>
<!--DAML+OIL Language, version 03/2001-->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
         xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
         xmlns="http://ontology.coginst.uwf.edu/ActorAttribute.daml#">
    
    <daml:Ontology rdf:about="">
        <daml:versionInfo>
            $ http://ontology.coginst.uwf.edu/ActorAttribute.daml $
        </daml:versionInfo>
        <rdfs:comment>
            An ontology created by Andrzej Uszok (auszok@ai.uwf.edu).
        </rdfs:comment>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Entity.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml"/>
    </daml:Ontology>

<!--Section with property definitions--> 

    <daml:UnambigousProperty rdf:ID="hasAttribute">
        <rdfs:domain rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml#Actor"/> 
        <rdfs:range rdf:resource="#Attribute"/> 
    </daml:UnambigousProperty>

    <daml:ObjectProperty rdf:ID="isAssociatedWith">
        <daml:inverseOf rdf:resource="#hasAttribute"/>
    </daml:ObjectProperty> 

    <daml:Property rdf:ID="hasValue">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:domain rdf:resource="#Attribute"/>
    </daml:Property>

<!--Section with class definitions--> 

    <daml:Class rdf:ID="Attribute">
        <rdfs:subClassOf rdf:resource="http://ontology.coginst.uwf.edu/Entity.daml#ComputingEntity"/>
    </daml:Class>

</rdf:RDF>
