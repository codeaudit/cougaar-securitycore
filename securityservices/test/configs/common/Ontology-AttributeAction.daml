<?xml version='1.0'?>
<!--DAML+OIL Language, version 03/2001-->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
         xmlns:daml="http://www.daml.org/2001/03/daml+oil#" 
         xmlns="http://ontology.coginst.uwf.edu/AttributeAction.daml#">
    
    <daml:Ontology rdf:about="">
        <daml:versionInfo>
            $ http://ontology.coginst.uwf.edu/AttributeAction.daml $
        </daml:versionInfo>
        <rdfs:comment>
            An ontology created by Andrzej Uszok (auszok@ai.uwf.edu).
        </rdfs:comment>
        <daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Action.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/ActorAttribute.daml"/>
    </daml:Ontology>

<!-- Section with property definitions--> 

   <daml:ObjectProperty rdf:ID="accessedAgent">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#accessedEntity"/>
        <rdfs:domain rdf:resource="#AccessAttributeAction"/> 
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml#Actor"/> 
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="accessedAttribute">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#accessedEntity"/>
        <rdfs:domain rdf:resource="#AccessAttributeAction"/> 
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/ActorAttribute.daml#Attribute"/> 
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="touchedAgent">
        <rdfs:subPropertyOf rdf:resource="#accessedAgent"/>
        <rdfs:domain rdf:resource="#ChangeAttributeAction"/> 
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="changedAttribute">
        <rdfs:subPropertyOf rdf:resource="#accessedAttribute"/>
        <rdfs:domain rdf:resource="#ChangeAttributeAction"/>  
    </daml:ObjectProperty>

    <daml:DatatypeProperty rdf:ID="hasNewValue">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#hasContext"/>
        <rdfs:domain rdf:resource="#ChangeAttributeAction"/>
    </daml:DatatypeProperty>

<!-- Section with class definitions--> 

    <daml:Class rdf:ID="AccessAttributeAction">
        <rdfs:subClassOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#AccessAction"/>
    </daml:Class>

    <daml:Class rdf:ID="ChangeAttributeAction">
        <rdfs:subClassOf rdf:resource="#AccessAttributeAction"/>
    </daml:Class>

    <daml:Class rdf:ID="DiscloseAttributeAction">
        <rdfs:subClassOf rdf:resource="#AccessAttributeAction"/>
        <rdfs:subClassOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#CommunicationAction"/>
    </daml:Class>

</rdf:RDF>
