# Adds Servlet to test sending messages
#

plugin = "org.cougaar.core.security.test.message.SendMessageComponent"

society.each_agent do |agent|
  agent.add_component do |c|
    c.classname = plugin
  end
end

society.each_node_agent do |agent|
  agent.add_component do |c|
    c.classname = plugin
  end
end

