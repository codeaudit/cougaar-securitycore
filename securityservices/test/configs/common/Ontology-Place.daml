<?xml version='1.0'?>
<!--DAML+OIL Language, version 03/2001-->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
         xmlns:daml="http://www.daml.org/2001/03/daml+oil#" 
         xmlns="http://ontolgy.coginst.uwf.edu/Place.daml#">
    
    <daml:Ontology rdf:about="">
        <daml:versionInfo>
            $ http://ontolgy.coginst.uwf.edu/Place.daml $
        </daml:versionInfo>
        <rdfs:comment>
            An ontology created by Andrzej Uszok (auszok@ai.uwf.edu).
        </rdfs:comment>
        <daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Group.daml"/>
    </daml:Ontology>

<!--Section with property definitions--> 

    <daml:TransitiveProperty rdf:ID="contains">
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Group.daml#hasMember"/>
        <rdfs:domain rdf:resource="#Place"/>
        <rdfs:range rdf:resource="#Place"/> 
    </daml:TransitiveProperty>

    <daml:ObjectProperty rdf:ID="connects">
        <rdfs:domain rdf:resource="#Link"/>
        <rdfs:range rdf:resource="#Place"/> 
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="hasPlace">
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Group.daml#isMemberOf"/>
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:domain rdf:resource="http://www.daml.org/2001/03/daml+oil#Thing"/>
        <rdfs:range rdf:resource="#Place"/> 
    </daml:ObjectProperty>

<!--Section with class definitions--> 

    <daml:Class rdf:ID="Place">
        <rdfs:subClassOf rdf:resource="http://ontology.coginst.uwf.edu/Group.daml#Group"/>
    </daml:Class>
    
    <daml:Class rdf:ID="Link">
        <rdfs:subClassOf>
            <daml:Restriction daml:maxCardinality="2">
                <daml:onProperty rdf:resource="#connects"/>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="PhysicalPlace">
        <rdfs:subClassOf rdf:resource="#Place"/>
    </daml:Class>

    <daml:Class rdf:ID="ComputationalPlace">
        <rdfs:subClassOf rdf:resource="#Place"/>
    </daml:Class>

    <daml:Class rdf:about="#Place">
        <daml:disjointUnionOf rdf:parseType="daml:collection">
            <daml:Class rdf:about="#PhysicalPlace"/> 
            <daml:Class rdf:about="#ComputationalPlace"/> 
        </daml:disjointUnionOf>
    </daml:Class>

    <daml:Class rdf:ID="UnknownPhysicalPlace">
        <rdfs:subClassOf rdf:resource="#PhysicalPlace"/>
    </daml:Class>

    <daml:Class rdf:ID="PhysicalPlaceGroup">
        <rdfs:subClassOf rdf:resource="#PhysicalPlace"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass rdf:resource="#PhysicalPlace"/>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="UnknownComputationalPlace">
        <rdfs:subClassOf rdf:resource="#ComputationalPlace"/>
    </daml:Class>

    <daml:Class rdf:ID="ComputationalPlaceGroup">
        <rdfs:subClassOf rdf:resource="#ComputationalPlace"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass rdf:resource="#ComputationalPlace"/>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>
    
    <daml:Class rdf:ID="Host">
        <rdfs:subClassOf rdf:resource="#ComputationalPlace"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="#SystemEnvironmentGroup"/> 
                            <daml:Class rdf:about="#SystemEnvironment"/> 
                            <daml:Class rdf:about="#VirtualMachineGroup"/> 
                            <daml:Class rdf:about="#VirtualMachine"/> 
                            <daml:Class rdf:about="#ContainerGroup"/>
                            <daml:Class rdf:about="#Container"/>
                        </daml:unionOf>
                    </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="HostGroup">
        <rdfs:subClassOf rdf:resource="#ComputationalPlaceGroup"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="#Host"/> 
                            <daml:Class rdf:about="#SystemEnvironmentGroup"/> 
                            <daml:Class rdf:about="#SystemEnvironment"/> 
                            <daml:Class rdf:about="#VirtualMachineGroup"/> 
                            <daml:Class rdf:about="#VirtualMachine"/> 
                            <daml:Class rdf:about="#ContainerGroup"/>
                            <daml:Class rdf:about="#Container"/>
                        </daml:unionOf>
                    </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="SystemEnvironment">
        <rdfs:subClassOf rdf:resource="#ComputationalPlace"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="#VirtualMachineGroup"/> 
                            <daml:Class rdf:about="#VirtualMachine"/> 
                            <daml:Class rdf:about="#ContainerGroup"/>
                            <daml:Class rdf:about="#Container"/>
                        </daml:unionOf>
                    </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="SystemEnvironmentGroup">
        <rdfs:subClassOf rdf:resource="#ComputationalPlaceGroup"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="#SystemEnvironment"/> 
                            <daml:Class rdf:about="#VirtualMachineGroup"/> 
                            <daml:Class rdf:about="#VirtualMachine"/> 
                            <daml:Class rdf:about="#ContainerGroup"/>
                            <daml:Class rdf:about="#Container"/>
                        </daml:unionOf>
                    </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="VirtualMachine">
        <rdfs:subClassOf rdf:resource="#ComputationalPlace"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="#ContainerGroup"/>
                            <daml:Class rdf:about="#Container"/>
                        </daml:unionOf>
                    </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>
    
    <daml:Class rdf:ID="VirtualMachineGroup">
        <rdfs:subClassOf rdf:resource="#ComputationalPlaceGroup"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="#VirtualMachine"/> 
                            <daml:Class rdf:about="#ContainerGroup"/>
                            <daml:Class rdf:about="#Container"/>
                        </daml:unionOf>
                    </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="Container">
        <rdfs:subClassOf rdf:resource="#ComputationalPlace"/>
    </daml:Class>
    
    <daml:Class rdf:ID="ContainerGroup">
        <rdfs:subClassOf rdf:resource="#ComputationalPlaceGroup"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="#contains"/>
                <daml:toClass rdf:resource="#Container"/>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>
   
</rdf:RDF>