<?xml version='1.0'?>
<!--DAML+OIL Language, version 03/2001-->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
         xmlns:daml="http://www.daml.org/2001/03/daml+oil#" 
         xmlns="http://ontology.coginst.uwf.edu/Action.daml#">
    
    <daml:Ontology rdf:about="">
        <daml:versionInfo>
            $ http://ontology.coginst.uwf.edu/PolicyAction.daml $
        </daml:versionInfo>
        <rdfs:comment>
            An ontology created by Andrzej Uszok (auszok@ai.uwf.edu).
        </rdfs:comment>
        <daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Action.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Group.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Entity.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Policy.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Message.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Place.daml"/>
    </daml:Ontology>

<!--Section with property definitions-->

    <daml:ObjectProperty rdf:ID="involvedPolicy">
        <rdfs:subPropertyOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedOn"/>
        <rdfs:domain rdf:resource="#PolicyAction"/>
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Policy.daml#Policy"/>
    </daml:ObjectProperty>

    <daml:ObjectProperty rdf:ID="hasAction">
        <rdfs:domain rdf:resource="#DisclosingPolicyAction"/>
        <rdfs:range rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#Action"/>
    </daml:ObjectProperty>

<!--Section with class definitions-->

    <daml:Class rdf:ID="PolicyAction">
        <rdfs:subClassOf rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#Action"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedBy"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Actor.daml#DomainManager"/> 
                            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Actor.daml#Guard"/> 
                            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Actor.daml#Person"/> 
                         </daml:unionOf>
                     </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="AddPolicyAction">
        <rdfs:subClassOf rdf:resource="#PolicyAction"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedBy"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Actor.daml#Guard"/> 
                            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Actor.daml#Person"/> 
                         </daml:unionOf>
                     </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>
    
    <daml:Class rdf:ID="RemovePolicyAction">
        <rdfs:subClassOf rdf:resource="#PolicyAction"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedBy"/>
                <daml:toClass>
                    <daml:Class>
                        <daml:unionOf rdf:parseType="daml:collection">
                            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Actor.daml#Guard"/> 
                            <daml:Class rdf:about="http://ontology.coginst.uwf.edu/Actor.daml#Person"/> 
                         </daml:unionOf>
                     </daml:Class>
                </daml:toClass>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="DelegatePolicyAction">
        <rdfs:subClassOf rdf:resource="#AddPolicyAction"/>
    </daml:Class>

    <daml:Class rdf:ID="DisclosePolicyAction">
        <rdfs:subClassOf rdf:resource="#PolicyAction"/>
        <rdfs:subClassOf>
            <daml:Restriction>
                <daml:onProperty rdf:resource="http://ontology.coginst.uwf.edu/Action.daml#performedBy"/>
                <daml:hasClass rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml#Guard"/>
            </daml:Restriction>
        </rdfs:subClassOf>
    </daml:Class>

    <daml:Class rdf:ID="ForbidAction">
        <rdfs:subClassOf rdf:resource="#DisclosePolicyAction"/>
    </daml:Class>

    <daml:Class rdf:ID="GrantAuthorizationAction">
        <rdfs:subClassOf rdf:resource="#DisclosePolicyAction"/>
    </daml:Class>

    <daml:Class rdf:ID="ObligateAction">
        <rdfs:subClassOf rdf:resource="#DisclosePolicyAction"/>
    </daml:Class>

</rdf:RDF>
