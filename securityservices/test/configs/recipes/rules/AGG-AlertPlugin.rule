####################################################
# AGG-AlertPlugin.rule

society.each_agent do |agent|
  if (agent.name =~ /.*SecurityMnRManager.*/ ) or (agent.name == 'AGG-Agent') or (agent.name == "SCmrmanager")
    agent.add_component do |c|
      c.classname = "org.cougaar.lib.aggagent.plugin.AlertPlugin"
    end
  end
end
