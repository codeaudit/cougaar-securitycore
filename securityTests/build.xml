<project name="sri-automation-overlay" default="create_cougaar_overlay" basedir=".">

<target name="create_cougaar_overlay">
	<property name="zip" value="SRI_auto.zip"/>
	<delete dir="staging"/>			
  <mkdir dir="staging/csmart/assessment/"/>
	<copy todir="staging/csmart/assessment/lib/">
		<fileset dir="lib03"/>
	</copy>
	<copy todir="staging/csmart/assessment/scripts/">
		<fileset dir="scripts03"/>
	</copy>
	<zip zipfile="${zip}" basedir="staging"/>
</target>

</project>
  
