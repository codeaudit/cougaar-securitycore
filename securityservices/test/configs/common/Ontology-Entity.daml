<?xml version='1.0'?>
<!--DAML+OIL Language, version 03/2001-->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
         xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
         xmlns="http://ontology.coginst.uwf.edu/Entity.daml#">
    
    <daml:Ontology rdf:about="">
        <daml:versionInfo>
            $ http://ontology.coginst.uwf.edu/Entity.daml $
        </daml:versionInfo>
        <rdfs:comment>
            An ontology created by Andrzej Uszok (auszok@ai.uwf.edu).
        </rdfs:comment>
        <daml:imports rdf:resource="http://www.daml.org/2001/03/daml+oil"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Action.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Place.daml"/>
        <daml:imports rdf:resource="http://ontology.coginst.uwf.edu/Policy.daml"/>
    </daml:Ontology>

<!--Section with property definitions--> 

    <daml:ObjectProperty rdf:ID="isTargetForAction">
        <rdfs:domain rdf:resource="#Entity"/>
        <rdfs:range rdf:resource="http://www.daml.org/2001/03/daml+oil#Class"/> 
    </daml:ObjectProperty>
    
    <daml:DatatypeProperty rdf:ID="hasMaxDuration">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:domain rdf:resource="#DurationLimitedEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#integer"/> 
    </daml:DatatypeProperty>
    
    <daml:DatatypeProperty rdf:ID="hasMaxQuantity">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:domain rdf:resource="#QuantityLimitedEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#integer"/> 
    </daml:DatatypeProperty>

    <daml:DatatypeProperty rdf:ID="hasMaxRate">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:domain rdf:resource="#RateLimitedEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#integer"/> 
    </daml:DatatypeProperty>
    
    <daml:DatatypeProperty rdf:ID="hasMaxSize">
        <rdf:type rdf:resource="http://www.daml.org/2001/03/daml+oil#UniqueProperty"/>
        <rdfs:domain rdf:resource="#SizeLimitedEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#integer"/> 
    </daml:DatatypeProperty>

<!--Section with class definitions--> 

    <daml:Class rdf:ID="Entity">
        <daml:disjointWith rdf:resource="http://ontology.coginst.uwf.edu/Actor.daml#Actor"/>
    </daml:Class>

    <daml:Class rdf:ID="PhysicalEntity">
        <rdfs:subClassOf rdf:resource="#Entity"/>
    </daml:Class>

    <daml:Class rdf:ID="LogicalEntity">
        <rdfs:subClassOf rdf:resource="#Entity"/>
    </daml:Class>
    
    <daml:Class rdf:about="#Entity">
        <daml:disjointUnionOf rdf:parseType="daml:collection">
            <daml:Class rdf:about="#PhysicalEntity" /> 
            <daml:Class rdf:about="#LogicalEntity" /> 
        </daml:disjointUnionOf>
    </daml:Class>

    <daml:Class rdf:ID="ComputingEntity">
        <rdfs:subClassOf rdf:resource="#LogicalEntity"/>
    </daml:Class>

    <daml:Class rdf:ID="DurationLimitedEntity">
        <rdfs:subClassOf rdf:resource="#ComputingEntity"/>
    </daml:Class>
    
    <daml:Class rdf:ID="SizeLimitedEntity">
        <rdfs:subClassOf rdf:resource="#ComputingEntity"/>
    </daml:Class>
    
    <daml:Class rdf:ID="RateLimitedEntity">
        <rdfs:subClassOf rdf:resource="#ComputingEntity"/>
    </daml:Class>
    
    <daml:Class rdf:ID="QuantityLimitedEntity">
        <rdfs:subClassOf rdf:resource="#ComputingEntity"/>
    </daml:Class>
    
    <daml:Class rdf:ID="CPUEntity">
        <rdfs:subClassOf rdf:resource="#DurationLimitedEntity"/>
        <rdfs:subClassOf rdf:resource="#RateLimitedEntity"/>
    </daml:Class>
    
    <daml:Class rdf:ID="NetworkEntity">
        <rdfs:subClassOf rdf:resource="#RateLimitedEntity"/>
    </daml:Class>
    
    <daml:Class rdf:ID="DiskEntity">
        <rdfs:subClassOf rdf:resource="#SizeLimitedEntity"/>
        <rdfs:subClassOf rdf:resource="#RateLimitedEntity"/>
        <rdfs:subClassOf rdf:resource="#QuantityLimitedEntity"/>
    </daml:Class>
    
    <daml:Class rdf:ID="MemoryEntity">
        <rdfs:subClassOf rdf:resource="#SizeLimitedEntity"/>
    </daml:Class>
            
</rdf:RDF>
