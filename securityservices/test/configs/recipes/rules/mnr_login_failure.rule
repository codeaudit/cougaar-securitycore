# Rule to insert LoginFailureSensor to all node agents
# plugin = org.cougaar.core.security.monitoring.plugin.LoginFailureSensorn

plugin = "org.cougaar.core.security.monitoring.plugin.LoginFailureSensor"

society.each_node_agent() do |node_agent|
   node_agent.add_component do |c|
      c.classname = plugin
    end
end
