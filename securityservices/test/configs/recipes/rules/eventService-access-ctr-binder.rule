#!/usr/bin/ruby
####################################################
# Rule to insert EventAgentBinder

plugin = "org.cougaar.core.security.access.EventAgentBinderFactory"

society.each_agent(true) do |agent|
  agent.add_component do |c|
      c.insertionpoint = "Node.AgentManager.Agent.PluginManager.Binder"
      c.priority = "BINDER"
      c.classname = plugin
  end
end
